package com.sree.lambdatest;

import java.util.Comparator;
import java.util.List;
import java.util.stream.Collectors;

public class CheatSheet2 {

	public static void main(String[] args) {
		List<Employee2> employees = EmployeeDataBase.getAllEmployees();

		List<Employee2> lst = employees.stream().sorted(Comparator.comparing(Employee2::getSalary))
		.collect(Collectors.toList());
		
		System.out.println(lst.forEach(System.out::println));

	}

}
