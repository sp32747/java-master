package com.sree.practice;

import java.util.HashMap;
import java.util.Map;

public class ConcurrentHashMapTest extends Thread {

	static Map<Integer, String> hashMap = new HashMap();

	public static void main(String[] args) {

		hashMap.put(1, "A");
		hashMap.put(2, "B");
		hashMap.put(3, "C");

	}

}
