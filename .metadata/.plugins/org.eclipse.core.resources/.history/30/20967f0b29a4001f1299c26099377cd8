package com.sree.lambdatest;

import java.util.Comparator;
import java.util.List;
import java.util.stream.Collectors;

public class CheatSheet2 {

	public static void main(String[] args) {
		List<Employee2> employees = EmployeeDataBase.getAllEmployees();

	employees.stream().sorted(Comparator.comparing(e->e.getSalary())).collect(Collectors.toList()).iterator().forEachRemaining(System.out::println);
		
		
		
		
		
		//employees.stream().map(emp->emp.getName().toUpperCase()).collect(Collectors.toList()).forEach(System.out::println);
		
		//employeeList.stream().map(emp->emp.getName().toUpperCase()).collect(Collectors.toList()).forEach(System.out::println);

	}

}
