		employeeList.add(new Employee("sree", 30, 45000));
		employeeList.add(new Employee("nilu", 35, 670000));
		employeeList.add(new Employee("pan", 35, 900000));
		employeeList.add(new Employee("newt", 15, 780000));
		employeeList.add(new Employee("newt", 15, 1900000));
		return employeeList;

	}
}
