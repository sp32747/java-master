	public static void main(String[] args) {

		String input="i am srikanta panigrahi";
		
		//output of teh programme should be [i-am-srikanta-panigrahi]
		
		String[] inputArray = input.split(" ");
		
		StringJoiner joiner=new StringJoiner("[","-","]");
		
		
		
	}

}
